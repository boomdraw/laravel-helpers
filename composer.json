{
    "name": "boomdraw/laravel-helpers",
    "description": "Laravel heleprs",
    "keywords": [
        "boomdraw",
        "laravel",
        "laravel-helpers",
        "helpers"
    ],
    "homepage": "https://github.com/boomdraw/laravel-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Slesarenko",
            "email": "info@boomdraw.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "BoomDraw\\Helpers\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BoomDraw\\Helpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoomDraw\\Helpers\\HelpersServiceProvider"
            ]
        }
    },
    "suggest": {
        "barryvdh/laravel-ide-helper": "Allows to generate ide helpers with command ide-helper:all",
        "doctrine/dbal": "Needed to generate helpers for models"
    }
}
